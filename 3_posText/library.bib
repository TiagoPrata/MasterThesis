@article{Maurath1988,
abstract = {This paper presents a fundamental analysis of the stability of single-input/single-output (SISO), closed-loop systems with predictive controllers. The analysis can be used to calculate allowable modeling errors for a given system and controller. Design parameter selection guidelines for predictive controllers in SISO systems have been developed by considering performance, robustness, and ease of tuning. The performance and robustness of the resulting controllers are demonstrated in four numerical examples and compared to controllers designed using other parameter choices.},
author = {Maurath, Paul R. and Mellichamp, Duncan A. and Seborg, Dale E.},
doi = {10.1021/ie00078a011},
file = {:C$\backslash$:/Users/tiago/Google Drive/Estudos/MPC/Artigos/Predictive Controller Design for Single-Input Single-Output Systems.pdf:pdf},
issn = {0888-5885},
journal = {Industrial {\&} Engineering Chemistry Research},
month = {jun},
number = {6},
pages = {956--963},
title = {{Predictive controller design for single-input/single-output (SISO) systems}},
url = {http://pubs.acs.org/doi/abs/10.1021/ie00078a011},
volume = {27},
year = {1988}
}
