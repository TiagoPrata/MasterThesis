@inproceedings{Zeilinger2009,
abstract = {Recent results have suggested that online Model Predictive Control$\backslash$n(MPC) can be computed quickly enough to control fast sampled systems.$\backslash$nHigh-speed applications impose a hard real-time constraint on the$\backslash$nsolution of the MPC problem, which generally prevents the computation$\backslash$nof the optimal controller. In current approaches guarantees on feasibility$\backslash$nand stability are sacrificed in order to achieve a real-time setting.$\backslash$nIn this paper we develop a real-time MPC scheme based on robust MPC$\backslash$ndesign that recovers these guarantees while allowing for extremely$\backslash$nfast computation. We show that a simple warm-start optimization procedure$\backslash$nproviding an enhanced feasible solution guarantees feasibility and$\backslash$nstability for arbitrary time constraints. The proposed method can$\backslash$nbe practically implemented and efficiently solved for dynamic systems$\backslash$nof significant problem size. Implementation details for a real-time$\backslash$nrobust MPC method are provided that achieves computation times equal$\backslash$nto those reported for methods without guarantees. A 12-dimensional$\backslash$nproblem with 3 control inputs and a prediction horizon of 10 time$\backslash$nsteps is solved in 2msec with a performance deterioration less than$\backslash$n1{\%} and thereby allows for sampling rates of 500Hz.},
author = {Zeilinger, Melanie N. and Jones, Colin N. and Raimondo, Davide M. and Morari, Manfred},
booktitle = {Proceedings of the 48h IEEE Conference on Decision and Control (CDC) held jointly with 2009 28th Chinese Control Conference},
doi = {10.1109/CDC.2009.5400903},
file = {:C$\backslash$:/Users/tiago/Google Drive/Estudos/MPC/Artigos/Real-time MPC - Stability through robust MPC design.pdf:pdf},
isbn = {978-1-4244-3871-6},
issn = {01912216},
month = {dec},
pages = {3980--3986},
publisher = {IEEE},
title = {{Real-time MPC - Stability through robust MPC design}},
url = {http://ieeexplore.ieee.org/document/5400903/},
year = {2009}
}
