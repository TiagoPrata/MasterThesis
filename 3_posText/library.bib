@article{Bemporad2006,
abstract = {Model-based design is well recognized in industry as a systematic approach to the development, evaluation, and implementation of feedback controllers. Model predictive control (MPC) is a particular branch of model-based design: a dynamical model of the open-loop process is explicitly used to construct an optimization problem aimed at achieving the prescribed system's performance under specified restrictions on input and output variables. The solution of the optimization problem provides the feedback control action, and can be either computed by embedding a numerical solver in the real-time control code, or pre-computed off-line and evaluated through a lookup table of linear feedback gains. This paper reviews the basic ideas of MPC design, from the traditional linear MPC setup based on quadratic programming to more a advanced explicit and hybrid MPC, and highlights available software tools for the design, evaluation, code generation, and deployment of MPC controllers in real-time hardware platforms},
author = {Bemporad, Alberto},
doi = {10.1109/CDC.2006.377490},
file = {:C$\backslash$:/Users/tiago/Google Drive/Estudos/MPC/Artigos/Model Predictive Control Design - New Trends and Tools.pdf:pdf},
isbn = {1-4244-0171-2},
issn = {0191-2216},
journal = {Proceedings of the 45th IEEE Conference on Decision and Control},
number = {1},
pages = {6678--6683},
title = {{Model Predictive Control Design: New Trends and Tools}},
url = {http://ieeexplore.ieee.org/document/4178103/},
year = {2006}
}
@article{Levine2018,
abstract = {The framework of reinforcement learning or optimal control provides a mathe-matical formalization of intelligent decision making that is powerful and broadly applicable. While the general form of the reinforcement learning problem enables effective reasoning about uncertainty, the connection between reinforcement learn-ing and inference in probabilistic models is not immediately obvious. However, such a connection has considerable value when it comes to algorithm design: for-malizing a problem as probabilistic inference in principle allows us to bring to bear a wide array of approximate inference tools, extend the model in flexible and powerful ways, and reason about compositionality and partial observability. In this article, we will discuss how a generalization of the reinforcement learning or optimal control problem, which is sometimes termed maximum entropy rein-forcement learning, is equivalent to exact probabilistic inference in the case of deterministic dynamics, and variational inference in the case of stochastic dynam-ics. We will present a detailed derivation of this framework, overview prior work that has drawn on this and related ideas to propose new reinforcement learning and control algorithms, and describe perspectives on future research.},
archivePrefix = {arXiv},
arxivId = {arXiv:1805.00909v1},
author = {Levine, Sergey},
eprint = {arXiv:1805.00909v1},
file = {:C$\backslash$:/Users/tiago/Google Drive/Estudos/MPC/Artigos/Reinforcement learning and control as probabilistic inference.pdf:pdf},
title = {{Reinforcement Learning and Control as Probabilistic Inference: Tutorial and Review}},
url = {https://arxiv.org/pdf/1805.00909.pdf},
year = {2018}
}
@phdthesis{Santoro2011,
abstract = {Controle preditivo baseado em modelo (MPC) recebeu ampla aceita{\c{c}}{\~{a}}o na ind{\'{u}}stria qu{\'{i}}mica nos {\'{u}}ltimos 30 anos. O funcionamento b{\'{a}}sico dessa t{\'{e}}cnica {\'{e}} a utiliza{\c{c}}{\~{a}}o de um modelo para calcular o comportamento de uma planta em fun{\c{c}}{\~{a}}o das entradas que ela receberia nos pr{\'{o}}ximos instantes. Define-se um objetivo, cuja principal contribui{\c{c}}{\~{a}}o {\'{e}} dada por uma medida da dist{\^{a}}ncia entre a condi{\c{c}}{\~{a}}o predita da planta e um valor desejado previamente estipulado. Esse objetivo pode incluir ainda, por exemplo, penaliza{\c{c}}{\~{o}}es sobre o esfor{\c{c}}o de controle necess{\'{a}}rio para levar a planta a uma condi{\c{c}}{\~{a}}o mais pr{\'{o}}xima do desej{\'{a}}vel. S{\~{a}}o incorporadas restri{\c{c}}{\~{o}}es como limites f{\'{i}}sicos da planta e dos atuadores e formula-se um problema de otimiza{\c{c}}{\~{a}}o, buscando o ponto {\'{o}}timo dessa fun{\c{c}}{\~{a}}o objetivo e respeitando as restri{\c{c}}{\~{o}}es. Neste trabalho {\'{e}} abordado o problema de controle preditivo baseado em modelo para sistemas que apresentem integradores e/ou tempos mortos. Estes elementos tornam mais dif{\'{i}}cil o controle de processos baseado apenas em t{\'{e}}cnicas cl{\'{a}}ssicas. Apresenta-se aqui um modelo em espa{\c{c}}o de estados que permite a representa{\c{c}}{\~{a}}o dessas din{\^{a}}micas de modo suficientemente preciso. A formula{\c{c}}{\~{a}}o de modelo apresentada permite ainda a incorpora{\c{c}}{\~{a}}o de informa{\c{c}}{\~{o}}es sobre dist{\'{u}}rbios medidos. {\'{E}} feita uma demonstra{\c{c}}{\~{a}}o da estabilidade desse controlador quando o modelo por ele utilizado {\'{e}} id{\^{e}}ntico ao comportamento real da planta. Numa aplica{\c{c}}{\~{a}}o real do controlador proposto, seria necess{\'{a}}rio estimar os estados da planta a partir das medidas das sa{\'{i}}das. Em geral, utiliza-se um Filtro de Kalman para realizar esta tarefa. S{\~{a}}o estudados aqui os efeitos que a presen{\c{c}}a desse filtro teria sobre o desempenho do sistema em malha fechada. {\'{E}} proposto um observador baseado numa mudan{\c{c}}a heur{\'{i}}stica feita sobre o Filtro de Kalman e que permite, em certos casos, uma melhoria de desempenho. S{\~{a}}o apresentados os resultados de simula{\c{c}}{\~{o}}es de uma planta de {\'{o}}xido de etileno com o intuito de ilustrar a atua{\c{c}}{\~{a}}o do controlador est{\'{a}}vel desenvolvido e do observador proposto.},
